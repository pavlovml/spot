train_net: "models/VGG_CNN_M_1024/train.prototxt"

# display
display: 1
average_loss: 100

# snapshotting
#
# We've disabled the standard Caffe snapshots because Faster-RCNN uses a custom
# snapshot function. The snapshot prefix is still used, though.
snapshot: 0
snapshot_prefix: "vgg_cnn_m_1024_faster_rcnn"

# hyperparameters
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 350000
momentum: 0.9
weight_decay: 0.0005
iter_size: 2
